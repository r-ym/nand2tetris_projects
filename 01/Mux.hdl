/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel,out=sel2);
    Nand(a=a,b=sel2,out=a1);
	Nand(a=sel,b=b,out=b1);
	Nand(a=a1,b=b1,out=out);
}
